<%= form_with(model: @installed_product, local: true) do |form| %>
<div>
  <%= form.hidden_field :product_id %>
</div>
<div>
  <%= form.hidden_field :serial_number %>
</div>
<div>
  <%= form.hidden_field :installed_date %>
</div>
<div>
  <%= form.hidden_field :discount_price %>
</div>

<div>
  <P>name: <%= @installed_product.product_id %></P>
</div>
<div>
  <P>type_name: <%= @installed_product.serial_number %></P>
</div>
<div>
  <P>category: <%= @installed_product.installed_date %></P>
</div>
<div>
  <P>list_price: <%= @installed_product.discount_price %></P>
</div>
<div>
 <%= form.submit "submit"%>
</div>
<div>
  <%= form.submit 'back', name: 'back'%>
</div>
<% end %>
